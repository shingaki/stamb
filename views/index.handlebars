<div id="home">
    <div class="container">
        <div class="row">
            <div class="col-8 offset-2">
                <h1>{{msg}}</h1>
                <h2>This is our Home Page!</h2>
                <form class="clearfix mb-4" action="POST">
                    <div class="form-group" id="userName">
                        <label for="user">User Name</label>
                        <input type="text" id="user" class="form-control" aria-describedby="user" placeholder="User Name">
                        <p>{{user}}</p>
                    </div>
                    <div class="form-group">
                        <label for="password">Password</label>
                        <textarea class="form-control" id="password" aria-describedby="password" placeholder="Enter Password"></textarea>
                        <p>{{passwords}}</p>
                    </div>
                    <div class="form-group">
                        <label for="passwordConfirm">Confirm Password</label>
                        <textarea class="form-control" id="passwordConfirm" aria-describedby="password" name="confirm" placeholder="Enter Password"></textarea>
                        <p>{{passwordConfirm}}</p>
                    </div>
                    <div class="form-group">
                        <label for="email">Email</label>
                        <textarea class="form-control" id="email" aria-describedby="email" placeholder="Enter Email"></textarea>
                        <p>{{email}}</p>
                    </div>
                    <div class="form-group">
                        <label for="first">First Name</label>
                        <textarea class="form-control" id="first" aria-describedby="first" placeholder="Enter First Name"></textarea>
                        <p>{{first}}</p>
                    </div>
                    <div class="form-group">
                        <label for="last">Last Name</label>
                        <textarea class="form-control" id="last" aria-describedby="last" placeholder="Enter Last Name"></textarea>
                        <p>{{last}}</p>
                    </div>
                    <button id="submit" class="btn btn-primary float-right">Submit</button>
                </form>

            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>
